{
  "name": "cozy-konnector-paybyphone",
  "version": "1.2.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/paybyphone.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-run-dev",
    "standalone": "cozy-run-standalone",
    "pretest": "npm run clean",
    "test": "konitor testit .",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/paybyphone.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.11.2",
    "node-fetch": "^2.2.0",
    "querystring": "^0.2.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.5.3",
    "cozy-app-publish": "0.9.0",
    "cozy-jobs-cli": "1.5.1",
    "eslint": "5.7.0",
    "eslint-config-cozy-app": "1.1.3",
    "git-directory-deploy": "1.5.1",
    "husky": "1.1.2",
    "konitor": "0.9.0",
    "regenerator-runtime": "0.12.1",
    "svgo": "1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  }
}
